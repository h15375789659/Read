<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#F5E6C8" />
            <stroke
                android:width="1dp"
                android:color="#D4C4A8" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item
        android:bottom="20dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape android:shape="rectangle">
            <solid android:color="#5B4636" />
            <corners android:radius="2dp" />
        </shape>
    </item>
</layer-list>
